/* Space out content a bit */
body {
  padding-bottom: 20px;
  margin-top: 0;
  margin-left: 0;
  margin-right: 0;
}

/* Everything but the jumbotron gets side spacing for mobile first views */
.header,
.footer {
  padding-left: 10px;
  padding-right: 10px;
}

/* Custom page header */
.header {
  background:#797979;
  background-color: rgba(121, 122, 122, 0.9);
  padding:0px 00px 0 10px;
  height: 60px;
  float: left;
  width: 100%;
}
/* Make the masthead heading the same height as the navigation */
.header h2 {
  
  line-height: 5px;
  padding-bottom: 5px;
}

/* Custom page footer */
.footer {
  position:fixed;
  bottom:0;
  z-index:100;
  background:#797979;
  background-color: rgba(121, 122, 122, 0.9);
  padding: 0px 10px 0 10px;
  color:#fff;
  width:100%;
  left:0;
}


.loginFields{
  padding: 2px;
  background: #797979;
  background-color: rgba(121, 122, 122, 0.9);
  height: 35px;
  position: absolute;
  top: 2px;
  right: 20px;
  height: 35px;
  padding-top: 10px;
}

.userFields{
  border-radius: 2px;
}

.templateView{
  float: left;
}


/* css for left nav bar in feed and profile page */
.nav {
    line-height:30px;
    height:510px;
    width:100px;
    float:left;
    padding:15px; 
    border-right: 2px solid black; 
    
}

#feedPage {
    width:auto;
    float:left;
    padding:10px;  
}

#profilePage{
    width:auto;
    float:left;
    
    padding-right: 40px;
    padding-left: 300px;
    padding-top: 80px;
    padding-bottom: 20px;
}

button {
    border: 1px solid black;
    padding: 7px;
}

button.defaultBtn {
    width: 100px;
    font-weight: bold;
    margin-bottom: 10px;
}


#btnSignOut{
  padding: 2px;
  background-color: rgba(216, 221, 221, 0.9);
  height: 35px;
  position: absolute;
  top: 10px;
  right: 20px;
}

.profileElements{
  width: 300px;
  float: right;
}

.padding {
    padding-top: 10px;
    padding-right: 10px;
    padding-bottom: 10px;
    padding-left: 10px;
    border-style: dotted;
    border-width: 3px;
}

.usrimgSize{
  width: 60px;
}
.usrDelImg{
  width: 45px;
  float: right;
}

.feedValues{
    font-size: 35px;
    padding-left: 20px;
    padding-right: 20px;
    font-family: cursive;
    right: 100px;
    
}

.feedTime{
  right: 450px;
  position: absolute;
  padding-top: 30px;
  font-family: cursive;
}

.imgDimensions{
  float: center;
  width: 987px;
  height: 540px;
  padding-left: 180px;
}

.odd {
  background-color: red;
}
.even {
  background-color: blue; /* ng-class-odd="odd" ng-class-even="even" */
}